        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "Value": true,
              "__name__": true
            },
            "indexByName": {},
            "renameByName": {
              "namespace": "Namespace",
              "phase": "Phase",
              "rollout": "Rollout",
              "strategy": "Strategy"
            }
          }
        }
      ],
      "type": "table"
