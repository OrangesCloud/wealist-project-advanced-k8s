            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null }
            ]
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Phase" },
            "properties": [
              {
                "id": "custom.cellOptions",
                "value": {
                  "mode": "basic",
                  "type": "color-background"
                }
              },
              {
                "id": "mappings",
                "value": [
                  { "options": { "Healthy": { "color": "green", "index": 0, "text": "Healthy" } }, "type": "value" },
                  { "options": { "Progressing": { "color": "yellow", "index": 1, "text": "Progressing" } }, "type": "value" },
                  { "options": { "Degraded": { "color": "red", "index": 2, "text": "Degraded" } }, "type": "value" },
                  { "options": { "Paused": { "color": "blue", "index": 3, "text": "Paused" } }, "type": "value" }
