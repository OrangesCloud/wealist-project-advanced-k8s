        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "100 * (\n  sum(rate(board_service_http_requests_total{namespace=~\"$namespace\", status=\"5xx\"}[5m]) or vector(0))\n  + sum(rate(user_service_http_requests_total{namespace=~\"$namespace\", status=\"5xx\"}[5m]) or vector(0))\n  + sum(rate(chat_service_http_requests_total{namespace=~\"$namespace\", status=\"5xx\"}[5m]) or vector(0))\n  + sum(rate(noti_service_http_requests_total{namespace=~\"$namespace\", status=\"5xx\"}[5m]) or vector(0))\n  + sum(rate(storage_service_http_requests_total{namespace=~\"$namespace\", status=\"5xx\"}[5m]) or vector(0))\n  + sum(rate(video_service_http_requests_total{namespace=~\"$namespace\", status=\"5xx\"}[5m]) or vector(0))\n) / (\n  sum(rate(board_service_http_requests_total{namespace=~\"$namespace\"}[5m]) or vector(0))\n  + sum(rate(user_service_http_requests_total{namespace=~\"$namespace\"}[5m]) or vector(0))\n  + sum(rate(chat_service_http_requests_total{namespace=~\"$namespace\"}[5m]) or vector(0))\n  + sum(rate(noti_service_http_requests_total{namespace=~\"$namespace\"}[5m]) or vector(0))\n  + sum(rate(storage_service_http_requests_total{namespace=~\"$namespace\"}[5m]) or vector(0))\n  + sum(rate(video_service_http_requests_total{namespace=~\"$namespace\"}[5m]) or vector(0))\n)",
          "legendFormat": "",
          "refId": "A"
        }
