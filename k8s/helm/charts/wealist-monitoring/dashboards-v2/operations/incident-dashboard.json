        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [
            { "options": { "from": 0, "result": { "color": "green", "index": 0, "text": "HEALTHY" }, "to": 0.5 }, "type": "range" },
            { "options": { "from": 0.5, "result": { "color": "yellow", "index": 1, "text": "DEGRADED" }, "to": 5 }, "type": "range" },
            { "options": { "from": 5, "result": { "color": "red", "index": 2, "text": "CRITICAL" }, "to": 100 }, "type": "range" }
