        "tooltip": { "mode": "multi", "sort": "desc" }
      },
      "pluginVersion": "10.2.0",
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "(sum(rate({__name__=~\".*_service_http_requests_total\", namespace=~\"$namespace\", status=\"5xx\"}[1h]) or vector(0)) / sum(rate({__name__=~\".*_service_http_requests_total\", namespace=~\"$namespace\"}[1h]))) / (1 - 0.999)",
          "legendFormat": "Burn Rate (1h)",
          "refId": "A"
