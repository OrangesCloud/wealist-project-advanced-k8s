{
  "annotations": {
    "list": []
  },
  "description": "Service-specific Log Analysis - Deep dive into service logs",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 1,
      "title": "Service Health",
      "type": "row"
    },
    {
      "datasource": { "type": "loki", "uid": "${loki_datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 1 },
      "id": 2,
      "options": { "colorMode": "value", "graphMode": "area", "justifyMode": "auto", "orientation": "auto", "reduceOptions": { "calcs": ["sum"], "fields": "", "values": false }, "textMode": "auto" },
      "pluginVersion": "10.2.2",
      "targets": [
        {
          "datasource": { "type": "loki", "uid": "${loki_datasource}" },
          "expr": "sum(count_over_time({namespace=\"$namespace\", app=\"$service\"}[$__range]))",
          "legendFormat": "Total",
          "refId": "A"
        }
      ],
      "title": "Total Logs",
      "type": "stat"
    },
    {
      "datasource": { "type": "loki", "uid": "${loki_datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 10 }, { "color": "red", "value": 100 }] },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 1 },
      "id": 3,
      "options": { "colorMode": "value", "graphMode": "area", "justifyMode": "auto", "orientation": "auto", "reduceOptions": { "calcs": ["sum"], "fields": "", "values": false }, "textMode": "auto" },
      "pluginVersion": "10.2.2",
      "targets": [
        {
          "datasource": { "type": "loki", "uid": "${loki_datasource}" },
          "expr": "sum(count_over_time({namespace=\"$namespace\", app=\"$service\"} |~ \"(?i)error|panic|fatal\"[$__range]))",
          "legendFormat": "Errors",
          "refId": "A"
        }
      ],
      "title": "Errors",
      "type": "stat"
    },
    {
      "datasource": { "type": "loki", "uid": "${loki_datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 50 }, { "color": "orange", "value": 200 }] },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 1 },
      "id": 4,
      "options": { "colorMode": "value", "graphMode": "area", "justifyMode": "auto", "orientation": "auto", "reduceOptions": { "calcs": ["sum"], "fields": "", "values": false }, "textMode": "auto" },
      "pluginVersion": "10.2.2",
      "targets": [
        {
          "datasource": { "type": "loki", "uid": "${loki_datasource}" },
          "expr": "sum(count_over_time({namespace=\"$namespace\", app=\"$service\"} |~ \"(?i)warn\"[$__range]))",
          "legendFormat": "Warnings",
          "refId": "A"
        }
      ],
      "title": "Warnings",
      "type": "stat"
    },
    {
      "datasource": { "type": "loki", "uid": "${loki_datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 1 },
      "id": 5,
      "options": { "colorMode": "value", "graphMode": "area", "justifyMode": "auto", "orientation": "auto", "reduceOptions": { "calcs": ["mean"], "fields": "", "values": false }, "textMode": "auto" },
      "pluginVersion": "10.2.2",
      "targets": [
        {
          "datasource": { "type": "loki", "uid": "${loki_datasource}" },
          "expr": "sum(rate({namespace=\"$namespace\", app=\"$service\"}[5m])) * 60",
          "legendFormat": "Rate",
          "refId": "A"
        }
      ],
      "title": "Logs/min",
      "type": "stat"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 },
      "id": 6,
      "title": "Log Trends",
      "type": "row"
    },
    {
      "datasource": { "type": "loki", "uid": "${loki_datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "axisCenteredZero": false, "axisColorMode": "text", "axisLabel": "", "axisPlacement": "auto", "barAlignment": 0, "drawStyle": "bars", "fillOpacity": 100, "gradientMode": "none", "hideFrom": { "legend": false, "tooltip": false, "viz": false }, "lineInterpolation": "linear", "lineWidth": 1, "pointSize": 5, "scaleDistribution": { "type": "linear" }, "showPoints": "never", "spanNulls": false, "stacking": { "group": "A", "mode": "normal" }, "thresholdsStyle": { "mode": "off" } },
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] },
          "unit": "short"
        },
        "overrides": [
          { "matcher": { "id": "byRegexp", "options": ".*error.*|.*ERROR.*" }, "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }] },
          { "matcher": { "id": "byRegexp", "options": ".*warn.*|.*WARN.*" }, "properties": [{ "id": "color", "value": { "fixedColor": "yellow", "mode": "fixed" } }] },
          { "matcher": { "id": "byRegexp", "options": ".*info.*|.*INFO.*" }, "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }] },
          { "matcher": { "id": "byRegexp", "options": ".*debug.*|.*DEBUG.*" }, "properties": [{ "id": "color", "value": { "fixedColor": "blue", "mode": "fixed" } }] }
        ]
      },
      "gridPos": { "h": 6, "w": 24, "x": 0, "y": 6 },
      "id": 7,
      "options": { "legend": { "calcs": [], "displayMode": "list", "placement": "bottom", "showLegend": true }, "tooltip": { "mode": "multi", "sort": "desc" } },
      "pluginVersion": "10.2.2",
      "targets": [
        {
          "datasource": { "type": "loki", "uid": "${loki_datasource}" },
          "expr": "sum by (level) (count_over_time({namespace=\"$namespace\", app=\"$service\"} | json | level != \"\" [$__interval]))",
          "legendFormat": "{{level}}",
          "refId": "A"
        }
      ],
      "title": "Log Level Trend",
      "type": "timeseries"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 12 },
      "id": 8,
      "title": "Error Analysis",
      "type": "row"
    },
    {
      "datasource": { "type": "loki", "uid": "${loki_datasource}" },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 13 },
      "id": 9,
      "options": { "dedupStrategy": "none", "enableLogDetails": true, "prettifyLogMessage": true, "showCommonLabels": false, "showLabels": false, "showTime": true, "sortOrder": "Descending", "wrapLogMessage": true },
      "pluginVersion": "10.2.2",
      "targets": [
        {
          "datasource": { "type": "loki", "uid": "${loki_datasource}" },
          "expr": "{namespace=\"$namespace\", app=\"$service\"} |~ \"(?i)error|panic|fatal\"",
          "legendFormat": "",
          "refId": "A"
        }
      ],
      "title": "Recent Errors",
      "type": "logs"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 21 },
      "id": 10,
      "title": "All Logs",
      "type": "row"
    },
    {
      "datasource": { "type": "loki", "uid": "${loki_datasource}" },
      "gridPos": { "h": 12, "w": 24, "x": 0, "y": 22 },
      "id": 11,
      "options": { "dedupStrategy": "none", "enableLogDetails": true, "prettifyLogMessage": true, "showCommonLabels": false, "showLabels": false, "showTime": true, "sortOrder": "Descending", "wrapLogMessage": true },
      "pluginVersion": "10.2.2",
      "targets": [
        {
          "datasource": { "type": "loki", "uid": "${loki_datasource}" },
          "expr": "{namespace=\"$namespace\", app=\"$service\"} | json | level=~\"$level\"",
          "legendFormat": "",
          "refId": "A"
        }
      ],
      "title": "Log Stream",
      "type": "logs"
    }
  ],
