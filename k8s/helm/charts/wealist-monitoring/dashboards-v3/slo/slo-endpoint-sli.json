{
  "annotations": {
    "list": []
  },
  "description": "Endpoint SLI Tracking - Per-endpoint Service Level Indicators",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 1,
      "title": "Endpoint SLI Summary",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 1 },
      "id": 2,
      "options": { "colorMode": "value", "graphMode": "area", "justifyMode": "auto", "orientation": "auto", "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "textMode": "auto" },
      "pluginVersion": "10.2.2",
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "count(count by (http_route) (traces_spanmetrics_calls_total{service_name=~\"$service\",http_route!=\"\"}))",
          "legendFormat": "Endpoints",
          "refId": "A"
        }
      ],
      "title": "Tracked Endpoints",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "red", "value": null }, { "color": "green", "value": 1 }] },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 1 },
      "id": 3,
      "options": { "colorMode": "value", "graphMode": "area", "justifyMode": "auto", "orientation": "auto", "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "textMode": "auto" },
      "pluginVersion": "10.2.2",
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "count((1 - (sum(rate(traces_spanmetrics_calls_total{service_name=~\"$service\",http_route!=\"\",status_code=\"STATUS_CODE_ERROR\"}[1h])) by (http_route) / sum(rate(traces_spanmetrics_calls_total{service_name=~\"$service\",http_route!=\"\"}[1h])) by (http_route))) >= $slo_target)",
          "legendFormat": "Meeting SLO",
          "refId": "A"
        }
      ],
      "title": "Endpoints Meeting SLO",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "red", "value": 1 }] },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 1 },
      "id": 4,
      "options": { "colorMode": "value", "graphMode": "area", "justifyMode": "auto", "orientation": "auto", "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "textMode": "auto" },
      "pluginVersion": "10.2.2",
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "count((1 - (sum(rate(traces_spanmetrics_calls_total{service_name=~\"$service\",http_route!=\"\",status_code=\"STATUS_CODE_ERROR\"}[1h])) by (http_route) / sum(rate(traces_spanmetrics_calls_total{service_name=~\"$service\",http_route!=\"\"}[1h])) by (http_route))) < $slo_target)",
          "legendFormat": "Violating SLO",
          "refId": "A"
        }
      ],
      "title": "Endpoints Violating SLO",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 0.5 }, { "color": "red", "value": 1 }] },
          "unit": "s"
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 1 },
      "id": 5,
      "options": { "colorMode": "value", "graphMode": "area", "justifyMode": "auto", "orientation": "auto", "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "textMode": "auto" },
      "pluginVersion": "10.2.2",
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "max(histogram_quantile(0.95, sum(rate(traces_spanmetrics_duration_seconds_bucket{service_name=~\"$service\",http_route!=\"\"}[1h])) by (http_route, le)))",
          "legendFormat": "Max P95",
          "refId": "A"
        }
      ],
      "title": "Slowest Endpoint P95",
      "type": "stat"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 },
      "id": 6,
      "title": "Endpoint SLI Table",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "align": "auto", "cellOptions": { "type": "auto" }, "inspect": false },
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] }
        },
        "overrides": [
          { "matcher": { "id": "byName", "options": "Availability" }, "properties": [{ "id": "unit", "value": "percent" }, { "id": "thresholds", "value": { "mode": "absolute", "steps": [{ "color": "red", "value": null }, { "color": "yellow", "value": 99 }, { "color": "green", "value": 99.9 }] } }, { "id": "custom.cellOptions", "value": { "mode": "gradient", "type": "gauge" } }] },
          { "matcher": { "id": "byName", "options": "Error Rate" }, "properties": [{ "id": "unit", "value": "percentunit" }, { "id": "thresholds", "value": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 0.001 }, { "color": "red", "value": 0.01 }] } }] },
          { "matcher": { "id": "byName", "options": "P95 Latency" }, "properties": [{ "id": "unit", "value": "s" }, { "id": "thresholds", "value": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 0.5 }, { "color": "red", "value": 1 }] } }] },
          { "matcher": { "id": "byName", "options": "Request Rate" }, "properties": [{ "id": "unit", "value": "reqps" }] },
          { "matcher": { "id": "byName", "options": "SLO Status" }, "properties": [{ "id": "mappings", "value": [{ "options": { "match": "true", "result": { "color": "green", "index": 0, "text": "OK" } }, "type": "special" }, { "options": { "match": "false", "result": { "color": "red", "index": 1, "text": "VIOLATION" } }, "type": "special" }] }] }
        ]
      },
      "gridPos": { "h": 12, "w": 24, "x": 0, "y": 6 },
      "id": 7,
      "options": { "cellHeight": "sm", "footer": { "countRows": false, "fields": "", "reducer": ["sum"], "show": false }, "showHeader": true, "sortBy": [{ "desc": true, "displayName": "Request Rate" }] },
      "pluginVersion": "10.2.2",
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "(1 - (sum(rate(traces_spanmetrics_calls_total{service_name=~\"$service\",http_route!=\"\",status_code=\"STATUS_CODE_ERROR\"}[1h])) by (service_name, http_method, http_route) / sum(rate(traces_spanmetrics_calls_total{service_name=~\"$service\",http_route!=\"\"}[1h])) by (service_name, http_method, http_route))) * 100",
          "format": "table",
          "instant": true,
          "legendFormat": "__auto",
          "refId": "A"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "sum(rate(traces_spanmetrics_calls_total{service_name=~\"$service\",http_route!=\"\",status_code=\"STATUS_CODE_ERROR\"}[1h])) by (service_name, http_method, http_route) / sum(rate(traces_spanmetrics_calls_total{service_name=~\"$service\",http_route!=\"\"}[1h])) by (service_name, http_method, http_route)",
          "format": "table",
          "instant": true,
          "legendFormat": "__auto",
          "refId": "B"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "histogram_quantile(0.95, sum(rate(traces_spanmetrics_duration_seconds_bucket{service_name=~\"$service\",http_route!=\"\"}[1h])) by (service_name, http_method, http_route, le))",
          "format": "table",
          "instant": true,
          "legendFormat": "__auto",
          "refId": "C"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "sum(rate(traces_spanmetrics_calls_total{service_name=~\"$service\",http_route!=\"\"}[1h])) by (service_name, http_method, http_route)",
          "format": "table",
          "instant": true,
          "legendFormat": "__auto",
          "refId": "D"
        }
      ],
      "title": "Endpoint SLI Status (1-Hour)",
      "transformations": [
        { "id": "seriesToColumns", "options": { "byField": "http_route" } },
        { "id": "organize", "options": { "excludeByName": { "Time": true, "Time 1": true, "Time 2": true, "Time 3": true, "Time 4": true }, "indexByName": {}, "renameByName": { "Value #A": "Availability", "Value #B": "Error Rate", "Value #C": "P95 Latency", "Value #D": "Request Rate", "http_method": "Method", "http_method 1": "Method", "http_route": "Endpoint", "service_name": "Service", "service_name 1": "Service" } } }
      ],
      "type": "table"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 18 },
      "id": 8,
      "title": "Endpoint Availability Trend",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "axisCenteredZero": false, "axisColorMode": "text", "axisLabel": "", "axisPlacement": "auto", "barAlignment": 0, "drawStyle": "line", "fillOpacity": 10, "gradientMode": "none", "hideFrom": { "legend": false, "tooltip": false, "viz": false }, "lineInterpolation": "linear", "lineWidth": 1, "pointSize": 5, "scaleDistribution": { "type": "linear" }, "showPoints": "auto", "spanNulls": false, "stacking": { "group": "A", "mode": "none" }, "thresholdsStyle": { "mode": "line" } },
          "mappings": [],
          "max": 100,
          "min": 99,
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "red", "value": 99.9 }] },
          "unit": "percent"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 19 },
      "id": 9,
      "options": { "legend": { "calcs": ["mean", "min"], "displayMode": "table", "placement": "right", "showLegend": true }, "tooltip": { "mode": "multi", "sort": "desc" } },
      "pluginVersion": "10.2.2",
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "topk(10, (1 - (sum(rate(traces_spanmetrics_calls_total{service_name=~\"$service\",http_route!=\"\",status_code=\"STATUS_CODE_ERROR\"}[5m])) by (http_route) / sum(rate(traces_spanmetrics_calls_total{service_name=~\"$service\",http_route!=\"\"}[5m])) by (http_route))) * 100)",
          "legendFormat": "{{http_route}}",
          "refId": "A"
        }
      ],
      "title": "Top Endpoints by Request Rate - Availability",
      "type": "timeseries"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 27 },
      "id": 10,
      "title": "Endpoint Latency SLI",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "axisCenteredZero": false, "axisColorMode": "text", "axisLabel": "", "axisPlacement": "auto", "barAlignment": 0, "drawStyle": "line", "fillOpacity": 10, "gradientMode": "none", "hideFrom": { "legend": false, "tooltip": false, "viz": false }, "lineInterpolation": "linear", "lineWidth": 1, "pointSize": 5, "scaleDistribution": { "type": "linear" }, "showPoints": "auto", "spanNulls": false, "stacking": { "group": "A", "mode": "none" }, "thresholdsStyle": { "mode": "line" } },
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 0.5 }, { "color": "red", "value": 1 }] },
          "unit": "s"
        },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 28 },
      "id": 11,
      "options": { "legend": { "calcs": ["mean", "max"], "displayMode": "table", "placement": "right", "showLegend": true }, "tooltip": { "mode": "multi", "sort": "desc" } },
      "pluginVersion": "10.2.2",
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "topk(10, histogram_quantile(0.95, sum(rate(traces_spanmetrics_duration_seconds_bucket{service_name=~\"$service\",http_route!=\"\"}[5m])) by (http_route, le)))",
          "legendFormat": "{{http_route}}",
          "refId": "A"
        }
      ],
      "title": "Top Endpoints P95 Latency",
      "type": "timeseries"
    }
  ],
  "refresh": "30s",
  "schemaVersion": 38,
  "style": "dark",
  "tags": ["slo", "v3", "endpoint", "sli", "otel"],
  "templating": {
    "list": [
      {
        "current": { "selected": false, "text": "Prometheus", "value": "prometheus" },
        "hide": 0,
        "includeAll": false,
        "multi": false,
        "name": "datasource",
        "options": [],
        "query": "prometheus",
        "refresh": 1,
        "regex": "",
        "skipUrlSync": false,
        "type": "datasource"
      },
      {
        "allValue": ".*",
        "current": { "selected": true, "text": "All", "value": "$__all" },
        "datasource": { "type": "prometheus", "uid": "${datasource}" },
        "definition": "label_values(traces_spanmetrics_calls_total, service_name)",
        "hide": 0,
        "includeAll": true,
        "multi": true,
        "name": "service",
        "options": [],
        "query": { "query": "label_values(traces_spanmetrics_calls_total, service_name)", "refId": "PrometheusVariableQueryEditor-VariableQuery" },
        "refresh": 2,
        "regex": "",
        "skipUrlSync": false,
        "sort": 1,
        "type": "query"
      },
      {
        "current": { "selected": false, "text": "0.999", "value": "0.999" },
        "hide": 0,
        "includeAll": false,
        "multi": false,
        "name": "slo_target",
        "options": [
          { "selected": false, "text": "0.99", "value": "0.99" },
          { "selected": true, "text": "0.999", "value": "0.999" },
          { "selected": false, "text": "0.9999", "value": "0.9999" }
        ],
        "query": "0.99,0.999,0.9999",
        "skipUrlSync": false,
        "type": "custom"
      }
    ]
  },
  "time": { "from": "now-6h", "to": "now" },
  "timepicker": {},
  "timezone": "",
  "title": "SLO Endpoint SLI",
  "uid": "slo-endpoint-sli-v3",
  "version": 1,
  "weekStart": ""
}
